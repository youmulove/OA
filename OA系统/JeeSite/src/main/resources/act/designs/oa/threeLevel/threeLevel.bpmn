      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{flag=='y'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask5" name="退回" activiti:assignee="#{applyUserId}"></userTask>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="usertask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{flag=='n'}]]></conditionExpression>
    </sequenceFlow>
